{
  "name": "chipotle",
  "description": "Tinkering with React+Redux",
  "version": "0.0.0",
  "author": "Alexandre Maia <alexandre.maia@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cxz/chipotle"
  },
  "bugs": "https://github.com/cxz/chipotle/issues",
  "scripts": {
    "clean:build": "rm -rf build && mkdir build",
    "build:html": "node ./bin/buildHtml.js",
    "prestart": "rm -rf build",
    "start": "webpack-dev-server --host 0.0.0.0 --config ./config/webpack.config.dev.js --hot --inline --progress",
    "prebuild": "npm run clean:build && npm run build:html",
    "build": "NODE_ENV=production webpack -p --config ./config/webpack.config.prod.js --profile --colors",
    "lint": "eslint config src/js/**/*.js",
    "jscs": "jscs src/js/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
    "babel-core": "6.5.2",
    "babel-eslint": "4.1.8",
    "babel-loader": "6.2.3",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
    "copy-webpack-plugin": "0.3.3",
    "css-loader": "0.23.1",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.16.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "jscs": "2.10.1",
    "node-sass": "3.4.2",
    "react-hot-loader": "1.3.0",
    "redux-devtools": "3.1.1",
    "redux-devtools-dock-monitor": "1.1.0",
    "redux-devtools-log-monitor": "1.0.4",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.13",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "classnames": "2.2.3",
    "humps": "^0.6.0",
    "isomorphic-fetch": "^2.1.1",
    "lodash": "4.5.0",
    "metrics-graphics": "2.9.0",
    "normalizr": "^2.0.0",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-redux": "4.4.0",
    "react-router": "2.0.0",
    "react-router-redux": "^4.0.0-rc.1",
    "redux": "3.3.1",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.3",
    "reselect": "2.5.2"
  }
}
